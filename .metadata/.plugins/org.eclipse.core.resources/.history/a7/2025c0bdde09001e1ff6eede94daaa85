    private IPersonInfoService personInfoService;

    @Test
    public void shouldRetrieveCorrectPersonInfo() {
<span class="fc" id="L22">	List&lt;PersonInfo&gt; personInfoResult = personInfoService.getPersonInfo(&quot;John&quot;, &quot;Boyd&quot;);</span>

<span class="fc" id="L24">	assertEquals(personInfoResult.get(0).getFirstName(), &quot;John&quot;);</span>
