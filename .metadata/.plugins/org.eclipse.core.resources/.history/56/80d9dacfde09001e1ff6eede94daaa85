package com.openclassroom.projet5.model.status;


import java.util.List;

import com.openclassroom.projet5.model.Persons;


//childAlert
public class ChildAlert {
    public String firstName;
    public String lastName;
    public List<Persons> familyMembers;

    public String getFirstName() {
	return firstName;
    }

    public void setFirstName(String firstName) {
	this.firstName = firstName;
    }

    public String getLastName() {
	return lastName;
    }

    public void setLastName(String lastName) {
	this.lastName = lastName;
    }

    public List<Persons> getFamilyMembers() {
	return familyMembers;
    }

    public void setFamilyMembers(List<Persons> familyMembers) {
	this.familyMembers = familyMembers;
    }

    public ChildAlert(String firstName, String lastName, List<Persons> familyMembers) {
	super();
	this.firstName = firstName;
	this.lastName = lastName;
	this.familyMembers = familyMembers;
    }

}

